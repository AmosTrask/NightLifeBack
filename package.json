{
  "name": "pillink",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.ts",
  "scripts": {
    "start": "npm run build && npm run init-db && node dist/server.js",
    "init-db": "ts-node ./scripts/run-init-db.ts",
    "serve": "npm run build && concurrently --kill-others \"npm run build-watch\" \"npm run serve-debug\"",
    "build": "tsc",
    "build-watch": "tsc -w",
    "serve-debug": "nodemon --inspect dist/server.js",
    "lint": "tslint --project .",
    "coverage": "npm run init-db && jest --forceExit --detectOpenHandles --coverage",
    "test": "jest --forceExit --detectOpenHandles"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/mongodb": "^3.1.9",
    "@types/supertest": "^2.0.5",
    "bcrypt": "^3.0.1",
    "concurrently": "^3.6.1",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-validator": "^5.3.0",
    "jest": "^23.5.0",
    "jsonwebtoken": "^8.3.0",
    "mongodb": "^3.1.6",
    "nodemon": "^1.18.3",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  }
}
